{
  "name": "time-parser",
  "version": "6.0.0",
  "description": "Parse the time tracking records of open source app, Tockler.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . \"Time Parser\" --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=../\"Time Parser\" --version-string.CompanyName=CP --version-string.FileDescription=CP --version-string.ProductName=\"Tockler Time Parser\"",
    "create-installer": "npm run package-win && cscript export_details.vbs && node build_installer.js"
  },
  "author": "Chris Poche",
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.6.1",
    "electron-packager": "^15.4.0",
    "electron-wix-msi": "^3.2.0"
  },
  "dependencies": {
    "electron-context-menu": "^3.1.1",
    "sqlite3": "^5.0.2"
  }
}
